{
  "name": "curso-protractor-tat",
  "version": "1.0.0",
  "description": "Projeto para o curso de Protractor da Escola TAT",
  "scripts": {
    "lint": "npx standard",
    "lint:fix": "npx standard --fix",
    "pretest:github": "webdriver-manager update --versions.chrome 96.0.4664.45",
    "pretest:local": "webdriver-manager update",
    "test": "npm run test:chrome && npm run test:firefox",
    "test:chrome": "protractor configurations/chromeConfig.js",
    "test:firefox": "protractor configurations/firefoxConfig.js"
  },
  "standard": {
    "globals": [
      "beforeAll",
      "beforeEach",
      "browser",
      "by",
      "describe",
      "element",
      "expect",
      "it"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agsvensson/CursoProtractor_TAT.git"
  },
  "keywords": [
    "protractor",
    "testing",
    "e2e"
  ],
  "author": "Arthur Svensson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agsvensson/CursoProtractor_TAT/issues"
  },
  "homepage": "https://github.com/agsvensson/CursoProtractor_TAT#readme",
  "devDependencies": {
    "faker": "^5.5.3",
    "protractor": "^7.0.0",
    "protractor-helper": "^4.2.1",
    "standardjs": "^1.0.0-alpha"
  }
}
